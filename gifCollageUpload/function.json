{
  "disabled": false,
  "bindings": [
    {
      "type": "eventGridTrigger",
      "direction": "in",
      "name": "eventGridEvent"
    },
    {
      "type": "signalR",
      "direction": "out",
      "name": "signalRGif",
      "hubName": "gifcollage",
      "connectionStringSetting": "AzureSignalRConnectionString"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "gifUrl",
      "databaseName": "gifcollage-prod",
      "collectionName": "gifUrl",
      "createIfNotExists": true,
      "connectionStringSetting": "COSMOS_DB_CONNECTION"
    }
  ],
  "entryPoint": "default",
  "scriptFile": "../src/functions/gif-collage-upload/index.js"
}